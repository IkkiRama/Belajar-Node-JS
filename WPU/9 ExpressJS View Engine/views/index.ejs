<h1>Hello World</h1>
<p>Welcome to my Website</p>
<% const nama = "Rifki Romadhan" %>
<p>Hai, nama saya adalah <%= nama %> dan saya sekarang berumur <%= umur %> </p>

<h1>Daftar Mahasiswa</h1>
<% if (mahasiswa.length === 0) { %>
<p>Tidak ada daftar mahasiswa</p>
<% } else { %>

<% mahasiswa.forEach(mhs => { %>
<ul>
    <li>Nama : <%= mhs.nama %> </li>
    <li>Jurusan : <%= mhs.jurusan %> </li>
</ul>
<% }) %>

<% } %>